<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_revatua" model="ir.ui.view">
        <field name="name">product.template.product.form.revatua</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <div name="options" position="after">
                <div>
                    <field name="est_contenant"/>
                    <label for="est_contenant"/>
                </div>
                <div>
                    <field name="matiere_dangereuse"/>
                    <label for="matiere_dangereuse"/>
                </div>
            </div>
            <field name="categ_id" position="after">
                <field name="nomenclaturepfcustoms_id"/>
            </field>
        </field>
    </record>
</odoo>
